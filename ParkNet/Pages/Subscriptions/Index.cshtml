@page
@model ParkNet.Pages.Subscriptions.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Subscription[0].User)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Subscription[0].ParkingLot)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Subscription[0].SubscriptionType)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Subscription[0].IncialDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Subscription[0].EndTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Subscription[0].Active)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Subscription) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.User.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ParkingLot.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SubscriptionType)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IncialDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EndTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Active)
            </td>          
        </tr>
}
    </tbody>
</table>
